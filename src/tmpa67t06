struct Poet {
  name: String,
}

impl Poet {

  fn new(name: &str) -> Poet {
    Poet {
      name: name.to_string(),
    }
  }
}

fn main() {
  let p1 = Poet::new("Anne Sexton");
  let p2 = Poet::new("Richard Wilbur");
  let p3 = Poet::new("W");
  let p4 = Poet::new("");
  let p5 = Poet::new("")
}
